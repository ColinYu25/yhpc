        <action name="createDangerGorverInit" class="com.safetys.nbyhpc.web.action.DangerGorverAction" method="createDangerGorverInit">
            <result type="freemarker">/template/danger_gorver/danger_gorver_info.ftl</result>
        </action>
        <action name="checkDangerGorverInit" class="com.safetys.nbyhpc.web.action.DangerGorverAction" method="checkDangerGorverInit">
