<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.safetys.nbyhpc.domain.model.ETLSynInfo" table="ETL_SYN_INFO">
        <id name="id" type="long">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="SynNo" type="long">
            <column name="SYN_NO" length="20" />
        </property>
        <property name="TabName" type="string">
            <column name="TAB_NAME" length="200" />
        </property>
        
    </class>
</hibernate-mapping>
